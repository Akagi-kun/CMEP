CompileFlags:
  Add: [-std=c++17, -Wall, -Wextra, -Wpedantic, -Wno-c++98-compat, -Wno-c++98-compat-pedantic, -Wno-switch-enum, -Wswitch, -Wno-date-time, -Wno-exit-time-destructors, -Wno-unsafe-buffer-usage, -Wno-cast-function-type-strict]
If:
  PathMatch: [.*\**.hpp, .*\**.cpp]
  PathExclude: external/.*
Diagnostics:
  ClangTidy:
    FastCheckFilter: None
---
Completion:
  AllScopes: No
InlayHints:
  Enabled: Yes
  ParameterNames: Yes
  DeducedTypes: No